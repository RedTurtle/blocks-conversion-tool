{
  "env": {
    "es2021": true
  },
  "extends": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-alert": 1,
    "no-debugger": 1,
    "prettier/prettier": [
      "error",
      { "trailingComma": "all", "singleQuote": true }
    ]
  }
}
